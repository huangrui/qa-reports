- content_for :version_navi do
  #version_navi
    = release_version_navigation(release.name)
    ul#report_filters.tabs selected="/recent"
      li.current
        a href="/recent" Recent
      li
        a href="/all" All

#index_page
  a#build_index_link href='/build_index' Build Index
  table#report_navigation style="display:none"
    thead
      tr.profiles
        th
          a.name href="#"
    tbody
      tr.profiles
        td.testsets
          div
            a.name
            input.inplace-edit style="display:none;"
            a.compare compare
            ul.products
              li
                a.name
                input.inplace-edit style="display:none;"

  - if user_signed_in?
    a#home_edit_link href='#'
    a#home_edit_done_link.small_btn href='#' Done

- content_for :view_specific_js do
  javascript:
    auth_token = "#{{form_authenticity_token}}";
  = javascript_include_tag 'build_index'
